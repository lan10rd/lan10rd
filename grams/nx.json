{
  "npmScope": "grams",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "common-ng": {
      "tags": []
    },
    "common-ns": {
      "tags": []
    },
    "host-api-ns": {
      "tags": []
    },
    "host-app-ng": {
      "tags": []
    },
    "play-api-ns": {
      "tags": []
    },
    "play-app-ng": {
      "tags": []
    },
    "play-app-ng-e2e": {
      "tags": [],
      "implicitDependencies": ["play-app-ng"]
    }
  }
}
